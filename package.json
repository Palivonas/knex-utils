{
  "name": "knex-utils",
  "version": "3.0.1",
  "description": "knex.js utils",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha ./test",
    "lint": "eslint . -f node_modules/eslint-html-reporter/reporter.js -o build/reports/eslint.html",
    "lint-to-console": "eslint .",
    "test-coverage": "nyc npm test"
  },
  "license": "MIT",
  "maintainers": [
    {
      "name": "igorsavin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/knex-utils.git"
  },
  "homepage": "https://github.com/kibertoad/knex-utils",
  "dependencies": {
    "knex": "0.14.3",
    "knex-tablecleaner": "^2.0.5",
    "validation-utils": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "sinon": "^4.3.0"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "src/**"
    ],
    "reporter": [
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/reports/coverage",
    "check-coverage": true,
    "lines": 50,
    "statements": 50,
    "functions": 66,
    "branches": 33
  }
}
